{% extends 'base.admin.html.twig' %}

{% block body %}
    <div id="details"></div>
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('details') }}
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    <script>
        const referential = "{{ app.request.get('referential') }}";
        const referential_api_version = "{{ path('referential', {
            version: referential_api_version,
            type: app.request.get('referential')
        })|escape('js') }}";
    </script>
    {{ encore_entry_script_tags('details') }}
{% endblock %}

